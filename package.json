{
    "name": "focus4-starter-kit",
    "version": "11.0.0",
    "description": "Starter kit pour Focus V4",
    "repository": "http://www.github.com/get-focus/focus4-starter-kit",
    "main": "src/index.ts",
    "scripts": {
        "build": "rm -rf static/* && webpack --mode=production",
        "cssgen": "focus4 cssgen src",
        "eslint": "eslint --ext .ts,.tsx src",
        "start": "rm -rf static/* && webpack serve --mode=development --devtool eval-source-map",
        "tslint": "tslint --project tsconfig.json --force"
    },
    "dependencies": {
        "focus4": "11.0.0"
    },
    "devDependencies": {
        "@focus4/tooling": "11.0.0",
        "style-loader": "3.2.1",
        "webpack-dev-server": "4.1.0"
    },
    "peerDependencies": {
        "@focus4/collections": "*",
        "@focus4/core": "*",
        "@focus4/forms": "*",
        "@focus4/layout": "*",
        "@focus4/stores": "*",
        "@focus4/styling": "*",
        "@focus4/toolbox": "*",
        "framer-motion": "*",
        "i18next": "*",
        "mobx": "*",
        "mobx-react": "*",
        "moment": "*",
        "numeral": "*",
        "react": "*",
        "react-dom": "*",
        "react-dnd": "*",
        "react-dnd-html5-backend": "*"
    },
    "author": "KleeGroup",
    "license": "MIT"
}
